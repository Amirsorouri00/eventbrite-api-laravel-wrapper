{
  "name": "amirsorouri00/eventbrite",
  "description": "Laravel PHP Facade/Wrapper for the Eventbrite Data API v3",
  "keywords": ["eventbrite", "api", "event", "laravel", "Amirsorouri00"],
  "homepage": "https://github.com/Amirsorouri00/eventbrite-api-laravel-wrapper",
  "license": "MIT",
  "authors": [
    {
      "name": "Marat Badykov",
      "email": "marat.badykov.555@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "scrutinizer/ocular": "^1.1",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Amirsorouri00\\Eventbrite\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Amirsorouri00\\Eventbrite\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Amirsorouri00\\Eventbrite\\EventbriteServiceProvider"
      ],
      "aliases": {
        "Eventbrite": "Amirsorouri00\\Eventbrite\\Facades\\Eventbrite"
      }
    }
  }
}
